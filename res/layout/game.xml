		<Button android:layout_width="wrap_content"  android:text="Pause" android:id="@+id/PauseButton" android:layout_height="match_parent"></Button>
		<Button android:layout_width="wrap_content"  android:text=" Check " android:id="@+id/CheckButton" android:layout_height="match_parent"></Button>
		<Button android:layout_width="wrap_content" android:text="Help" android:id="@+id/HelpButton" android:layout_height="match_parent"></Button>
		<Button android:layout_width="wrap_content"  android:text="Save" android:id="@+id/SaveButton" android:layout_height="match_parent"></Button>
	</LinearLayout>
	
<TableLayout android:layout_width="fill_parent" android:gravity="center" android:layout_height="wrap_content" android:id="@+id/SudokuGridSeparator" android:layout_below="@+id/ButtonsLayout">
